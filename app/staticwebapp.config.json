{
    "globalHeaders": {
      "content-security-policy": "default-src http: 'unsafe-eval' 'unsafe-inline'; object-src 'none'"
    },
    "navigationFallback": {
      "rewrite": "/index.html",
      "exclude": ["/img/*.{png,jpg,gif,webp}", "/css/*"]
    },
    "mimeTypes": {
      "custom": "text/html"
    }
  }